<h4><a href="#" class='back-to-list-link'>Sequences for Leaf</a> / <%= name %> by <%= owner %></h4>

<table id="sequence-grid" class="table">
  <tr>
    <th>Step #</th>
    <th>Step</th>
    <th>Narrative</th>
  </tr>
  <% _.each( steps, function(step) { %>
    <tr>
      <td class='step-number'><a href=""><%= step.step_number + 1 %></a></td>
      <td class='step-column'><%= step.step %></td>
      <td class='narrative-column'><%= step.narrative %></td>
    </tr>
  <%  }); %>
</table>
<div class='actions'>
    <button class='btn btn-default toggle-grid-button' href='#' >Show First Step</button>
</div>
